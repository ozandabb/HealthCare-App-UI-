<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Home.ActivityDermx">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <RelativeLayout
            android:id="@+id/relLayout0"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <!--            <include layout="@layout/home_top_toolbar"></include>-->
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/main_toolbar"
                android:background="@color/colorPrimary"
                app:titleTextColor="#ffffff"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

            </androidx.appcompat.widget.Toolbar>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relLayout01"
            android:layout_below="@id/relLayout0"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <include layout="@layout/layouts_top_tabs"></include>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relLayout02"
            android:layout_below="@+id/relLayout01"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <include layout="@layout/layout_center_viewpager"></include>
        </RelativeLayout>

        <include layout="@layout/layout_bottom_navigation_view"></include>
    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>